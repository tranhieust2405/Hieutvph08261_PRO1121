<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hieu.todoapp">

    <application
        android:name="com.example.hieu.todoapp.TodoApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.hieu.todoapp.activities.HomeActivity"></activity>
        <activity android:name="com.example.hieu.todoapp.activities.SignupActivity" />
        <activity android:name="com.example.hieu.todoapp.activities.MainActivity" />
        <activity android:name="com.example.hieu.todoapp.activities.LoginActivity" />
        <activity android:name="com.example.hieu.todoapp.activities.ManHinhChaoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>